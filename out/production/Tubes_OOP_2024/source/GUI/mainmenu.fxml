<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="source.GUI.ControllerMainMenu">
   <children>
      <Pane fx:id="pane" prefHeight="1078.0" prefWidth="1914.0">
         <children>
            <Button fx:id="startButton" layoutX="1218.0" layoutY="152.0" mnemonicParsing="false" onMouseClicked="#startGame" prefHeight="179.0" prefWidth="486.0" text="Start Game">
               <font>
                  <Font size="56.0" />
               </font>
            </Button>
            <Button fx:id="loadButton" layoutX="1220.0" layoutY="376.0" mnemonicParsing="false" prefHeight="179.0" prefWidth="486.0" text="Load Game">
               <font>
                  <Font size="56.0" />
               </font>
            </Button>
            <Button fx:id="plantListButton" layoutX="1222.0" layoutY="581.0" mnemonicParsing="false" onMouseClicked="#showPlantsList" prefHeight="143.0" prefWidth="234.0" text="Plants List">
               <font>
                  <Font size="40.0" />
               </font>
            </Button>
            <Button fx:id="zombieListButton" layoutX="1468.0" layoutY="576.0" mnemonicParsing="false" onMouseClicked="#showZombiesList" prefHeight="149.0" prefWidth="240.0" text="Zombies List">
               <font>
                  <Font size="32.0" />
               </font>
            </Button>
            <Button fx:id="helpButton" layoutX="1221.0" layoutY="736.0" mnemonicParsing="false" onMouseClicked="#showHelp" prefHeight="121.0" prefWidth="488.0" text="Help">
               <font>
                  <Font size="56.0" />
               </font>
            </Button>
            <Button fx:id="exitButton" layoutX="1219.0" layoutY="878.0" mnemonicParsing="false" onMouseClicked="#exitGame" prefHeight="121.0" prefWidth="488.0" text="Exit">
               <font>
                  <Font size="56.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </children>
</AnchorPane>
